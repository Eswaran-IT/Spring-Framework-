	<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
    
  <bean id="t" class="beans.Test">
      <!-- list fruits -->
  <property name="fruits">
      <list>
          <value>Apple</value>
          <value>Mango</value>          
      </list>      
  </property>
  		<!-- set cricketers -->
  <property name="cricketers">
      <set>
          <value>sachin</value>
          <value>sehwag</value>
          <value>sehwag</value>          
      </set>      
  </property>
  	<!-- Map countrycap -->
  <property name="countrycap">
      <map>
          <entry key="Ind" value="delhi"/>
          <entry key="Pak" value="islambad"/>
          <entry key="Lanka" value="colombo"/>          
      </map>      
  </property>
  <!-- Properties statecap -->
  <property name="statecap">
      <props>
          <prop key="Ts">hyd</prop>
          <prop key="Ka">bng</prop>
          <prop key="Tn">chn</prop>
      </props>      
  </property>
  
  </bean>    
    
</beans>